<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

       <template id="assets_frontend_snippets" inherit_id="website.assets_frontend" name="Website Snippets">
            <xpath expr="." position="inside">
                <script type="text/javascript" src="/custom/static/src/js/javascript.js"></script>
            </xpath>
        </template>

        <template id="moldeo_snipped_id" inherit_id="website.snippets" name="Moldeo Snippet">
            <xpath expr="//div[@id='snippet_structure']/div[@class='o_panel_body']" position="inside">
                <t t-snippet="custom.snippet_moldeo" t-thumbnail=""/>
            </xpath>
        </template>

        <template id="snippet_moldeo" name="Moldeo Snippet">
            <section>
                <div class="container">
                    <div class="row">
                        <div class="col-12 text-center">
                            <h2> Hola Mundo </h2>
                            <div>
                                <p> TESTO DE PRUEBA, POR FAVOR CAMBIAR</p>
                            </div>

                            <div id="products">
                                Aqui Apareceran los productos
                            </div>

                            <button class="btn btn-primary"> Boton Prueba</button>
                        </div>
                    </div>
                </div>

            </section>
        </template>
    </data>
</odoo>
