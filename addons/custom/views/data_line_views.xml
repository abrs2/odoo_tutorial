<?xml version="1.0"?>
<odoo>

    <record id="data_line_view_tree" model="ir.ui.view">
        <field name="name">data.line.tree</field>
        <field name="model">data.line</field>
        <field name="arch" type="xml">
            <tree string="Channel">
                <field name="code"/>
                <field name="cost"/>
            </tree>
        </field>
    </record>

    <record id="data_line_view_form" model="ir.ui.view">
        <field name="name">data.line.form</field>
        <field name="model">data.line</field>
        <field name="arch" type="xml">
            <form string="Data Line">
                <sheet>
                    <group>
                        <field name="code"/>
                        <field name="cost"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>

</odoo>